<UserControl x:Class="Kola.Controls.Tabs"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Kola.Controls"
             xmlns:kola="clr-namespace:Kola"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="800">
    <DockPanel>
        <Button Style="{StaticResource TitlebarButton}" Command="{x:Static kola:AppCommands.OpenTab}">&#xf067;</Button>
    </DockPanel>
</UserControl>
